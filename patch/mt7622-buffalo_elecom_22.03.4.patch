--- /dev/null
+++ b/target/linux/generic/hack-5.10/602-of_net-add-mac-address-ascii-support.patch
@@ -0,0 +1,48 @@
+--- linux-5.10.161/net/ethernet/eth.c.orig	2023-03-03 14:01:40.464426370 +0900
++++ linux-5.10.161/net/ethernet/eth.c	2023-03-03 16:11:46.452420508 +0900
+@@ -559,16 +559,37 @@
+ 	struct nvmem_cell *cell;
+ 	const void *mac;
+ 	size_t len;
++	void *mac_ascii;
+ 
+ 	cell = nvmem_cell_get(dev, "mac-address");
+-	if (IS_ERR(cell))
+-		return PTR_ERR(cell);
+-
+-	mac = nvmem_cell_read(cell, &len);
+-	nvmem_cell_put(cell);
+-
+-	if (IS_ERR(mac))
+-		return PTR_ERR(mac);
++	if (!IS_ERR(cell)) {
++		mac = nvmem_cell_read(cell, &len);
++		nvmem_cell_put(cell);
++		if (IS_ERR(mac))
++			return PTR_ERR(mac);
++	}
++	else {
++		cell = nvmem_cell_get(dev, "mac-address-ascii");
++		if (IS_ERR(cell))
++			return PTR_ERR(cell);
++		mac_ascii = nvmem_cell_read(cell, &len);
++		nvmem_cell_put(cell);
++		if (IS_ERR(mac_ascii))
++			return PTR_ERR(mac_ascii);
++		if (len != ETH_ALEN*2+5) {
++			kfree(mac_ascii);
++			return -EINVAL;
++		}
++		mac = kmalloc(ETH_ALEN, GFP_KERNEL);
++		if (!mac) {
++			kfree(mac_ascii);
++			return -ENOMEM;
++		}
++		len = sscanf(mac_ascii, "%2hhx:%2hhx:%2hhx:%2hhx:%2hhx:%2hhx",
++				&mac[0], &mac[1], &mac[2],
++				&mac[3], &mac[4], &mac[5]);
++		kfree(mac_ascii);
++	}
+ 
+ 	if (len != ETH_ALEN || !is_valid_ether_addr(mac)) {
+ 		kfree(mac);
